        // Bind the click events of the buttons to the functions that initiate the progress handling
        $(document).ready(function () {
            "use strict";
            $("#btnSubmit").bind("click", FormPostWithAjaxPOSTForm);
        });
        function FormPostWithAjaxPOSTForm() {
            "use strict";
            // To POST a form we can use the Javascript below, note the '$('FormPostWithAjax').serialize()' method in the data: element of the ajax call
            $.ajax({
                url: "/AjaxForms/FormPostWithAjaxUpdate/",
                type: "POST",
                data: $('FormPostWithAjax').serialize()
            })
                .done(function (data) { FormPostWithAjaxUpdateForm(data); })
                .fail(function () { alert('Form data not received.'); });

        }
        function FormPostWithAjaxUpdateForm(data) {
            $("#FormPostWithAjax_txtName").val(data.Name)
            $("#FormPostWithAjax_Name").val(data.Name)
            $("#FormPostWithAjax_Description").val(data.Description)
            $("#FormPostWithAjax_Value").val(data.Value)
