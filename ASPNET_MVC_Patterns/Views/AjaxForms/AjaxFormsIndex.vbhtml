@Code
    ViewData("Title") = "AjaxFormsIndex"
End Code

<h2>JQuery Ajax Post</h2>

<div>
    <p><a href="#" onclick="SaveData();">Save Data</a></p>
    <p><a href="#" onclick="SaveXML();">Save XML</a></p>
</div>

<h2>Form Post with Ajax</h2>

<div>
    @* Set up the form with a name and id but not an action *@
    @Using Html.BeginForm(Nothing, Nothing, FormMethod.Post, New With {.name = "FormPostWithAjax", .id = "FormPostWithAjax"})
        @<table>
            <tr><td><input type="text" id="FormPostWithAjax_txtName" value="@Model.Name" /></td></tr>
            <tr><td><input type="hidden" id="FormPostWithAjax_Name" name="Name" value="@Model.Name" /></td></tr>
            <tr><td><input type="hidden" id="FormPostWithAjax_Description" name="Description" value="@Model.Description" /></td></tr>
            <tr><td><input type="hidden" id="FormPostWithAjax_Value" name="Value" value="@Model.Value" /></td></tr>
            </table>
        @:<input id="btnSubmit" type="submit" />
    End Using
</div>

