            "use strict";
            var payload = "TEST_DATA_GOES_HERE";
            // To POST a simple string with no 'bad' characters we can use the Javascript below
            $.ajax({
                url: "/AjaxForms/JQueryAjaxPostSaveData/",
                type: "POST",
                data: { payload: payload }
            })
                .done(function () { alert('Application saved.'); })
                .fail(function () { alert('Application failed to save.'); });

